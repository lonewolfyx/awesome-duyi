{
    "extends": "@lonewolfyx/tsconfig/tsconfig.lib.json",
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "#/*": [
                "types/*"
            ]
        },
        "noUnusedLocals": false,
        "noUnusedParameters": false
    },
    "include": [
        "src/**/*",
        "types/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}
